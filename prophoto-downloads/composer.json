{
    "name": "prophoto/downloads",
    "description": "Bulk download system for ProPhoto - ZIP generation, progress tracking",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ProPhoto",
            "email": "dev@prophoto.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "zipper/zipper": "^1.0",
        "prophoto/contracts": "@dev",
        "prophoto/storage": "@dev",
        "prophoto/security": "@dev"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ProPhoto\\Downloads\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProPhoto\\Downloads\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProPhoto\\Downloads\\DownloadsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
