{
    "name": "prophoto/booking",
    "description": "Booking workflow engine for ProPhoto - requests, calendar sync, conflict detection",
    "type": "library",
    "license": "MIT",
    "authors": [{"name": "ProPhoto", "email": "dev@prophoto.com"}],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "google/apiclient": "^2.0",
        "prophoto/contracts": "@dev",
        "prophoto/tenancy": "@dev"
    },
    "require-dev": {"orchestra/testbench": "^9.0", "pestphp/pest": "^2.0"},
    "autoload": {"psr-4": {"ProPhoto\\Booking\\": "src/"}},
    "autoload-dev": {"psr-4": {"ProPhoto\\Booking\\Tests\\": "tests/"}},
    "extra": {"laravel": {"providers": ["ProPhoto\\Booking\\BookingServiceProvider"]}},
    "config": {"sort-packages": true, "allow-plugins": {"pestphp/pest-plugin": true}},
    "minimum-stability": "dev",
    "prefer-stable": true
}
