{
    "name": "prophoto/storage",
    "description": "Storage abstraction for ProPhoto - ImageKit integration, signed URLs, path conventions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ProPhoto",
            "email": "dev@prophoto.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "prophoto/contracts": "@dev",
        "prophoto/tenancy": "@dev"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ProPhoto\\Storage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProPhoto\\Storage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProPhoto\\Storage\\StorageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
