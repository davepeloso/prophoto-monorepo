{
    "name": "prophoto/debug",
    "description": "Debug and tracing tools for ProPhoto ingest pipeline",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ProPhoto",
            "email": "dev@prophoto.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ProPhoto\\Debug\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProPhoto\\Debug\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ProPhoto\\Debug\\DebugServiceProvider"
            ],
            "aliases": {
                "IngestDebug": "ProPhoto\\Debug\\Facades\\IngestDebug"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
